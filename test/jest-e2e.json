{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFiles": ["dotenv/config"],
  "moduleNameMapper": {
    "^@app$": "<rootDir>/../src/app.module",
    "^@api$": "<rootDir>/../src/api/api.module",
    "^@auth$": "<rootDir>/../src/auth/auth.module",
    "^@cache$": "<rootDir>/../src/cache/cache.module",
    "^@interfaces$": "<rootDir>/../src/common/interfaces/index",
    "^@utils$": "<rootDir>/../src/common/utils/index",
    "^@decorators$": "<rootDir>/../src/common/decorators/index",
    "^@filters$": "<rootDir>/../src/common/filters/index",
    "^@interceptors$": "<rootDir>/../src/common/interceptors/index",
    "^@guards$": "<rootDir>/../src/common/guards/index",
    "^@pipes$": "<rootDir>/../src/common/pipes/index",
    "^@type$": "<rootDir>/../src/common/types/index",
    "^@config$": "<rootDir>/../src/config/configuration",
    "^@database$": "<rootDir>/../src/database/database.module",
    "^@models/(.*)$": "<rootDir>/../src/models/$1/index",
    "^@rabbitmq$": "<rootDir>/../src/rabbitmq/rabbitmq.module",
    "^@redis$": "<rootDir>/../src/redis/redis.module",
    "^@slack$": "<rootDir>/../src/slack/slack.module"
  }
}
